<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
	<!-- inherit from product.template -->
		<record id="list_tree_view" model="ir.ui.view">
	    	<field name="name">product.product_template_tree_view.inherited</field>
	    	<field name="model">product.template</field>
	    	<field name="inherit_id" ref="product.product_template_tree_view"></field>
	    	<field name="arch" type="xml">
	    		<xpath expr="//field[@name='default_code']" position="after">
	    			<field name="barcode"></field>
	    		</xpath>
	   		</field>
		</record>
    </data>
</odoo>